# GitHub (personal)
Host github.com
    HostName github.com
    User git
    IdentitiesOnly no

# GitHub (work)
Host github-work
    HostName github.com
    User git
    IdentitiesOnly no

# GitLab (personal)
Host gitlab.com
    HostName gitlab.com
    User git
    IdentitiesOnly no

# GitLab (work)
Host gitlab-work
    HostName gitlab.com
    User git
    IdentitiesOnly no

# Remote machines
Host my-prod
    HostName 203.0.113.10
    User ubuntu
    IdentitiesOnly no

Host my-dev
    HostName dev.mycompany.internal
    User bjorn
    IdentitiesOnly no

# Catch-all
Host *
    UseKeychain yes
    AddKeysToAgent yes
    IdentitiesOnly no
    IdentityFile none
